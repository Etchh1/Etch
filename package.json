{
  "name": "etch",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "expo": "expo",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='<rootDir>/src/**/__integration__/**/*.test.[jt]s?(x)'",
    "test:e2e:build": "detox build --configuration ios.sim.debug",
    "test:e2e": "DETOX=true detox test --configuration ios.sim.debug",
    "test:e2e:android": "DETOX=true detox test --configuration android.emu.debug",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@expo/webpack-config": "^19.0.0",
