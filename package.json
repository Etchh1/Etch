    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "^14.0.2",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "prisma": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
